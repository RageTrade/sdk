{
  "version": "0.11.9",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "deployments",
    "src",
    "cli.js"
  ],
  "engines": {
    "node": ">=10"
  },
  "bin": {
    "rage": "./cli.js",
    "ragetrade": "./cli.js",
    "ragetradesdk": "./cli.js"
  },
  "scripts": {
    "start": "tsdx watch",
    "build:without-update-pools": "tsdx build",
    "build": "yarn clean && yarn build:without-update-pools && yarn update:pools",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "clean": "rm -rf dist",
    "prepare": "yarn build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "format": "prettier --check src test",
    "format:fix": "prettier --write src test",
    "update": "yarn update:flush && yarn update:deployments && node process-abi.js && yarn typechain && yarn build",
    "update:pkg": "yarn add @ragetrade/core@latest @ragetrade/vaults@latest",
    "update:flush": "rm -rf deployments src/typechain/core src/typechain/vaults src/typechain/uniswap && mkdir -p deployments/core deployments/vaults src/typechain/core src/typechain/vaults src/typechain/uniswap",
    "update:deployments": "cp -r node_modules/@ragetrade/core/deployments/ deployments/core && cp -r node_modules/@ragetrade/vaults/deployments/ deployments/vaults",
    "update:uniswap": "cp -r node_modules/@uniswap/v3-core/artifacts/contracts/UniswapV3Pool.sol/UniswapV3Pool.json deployments/uniswap/UniswapV3Pool.json",
    "update:pools": "node fetch-pools.js",
    "typechain": "yarn typechain:core && yarn typechain:tricrypto && yarn typechain:gmx && yarn typechain:uniswap:core && yarn typechain:uniswap:periphery && yarn format:fix",
    "typechain:repo-package": "typechain --target ethers-v5 --out-dir src/typechain/$REPO './node_modules/'\"$PACKAGE\"'/artifacts/**/*.sol/!(*.dbg.json)'",
    "typechain:core": "REPO=core PACKAGE=@ragetrade/core yarn typechain:repo-package",
    "typechain:tricrypto": "REPO=tricrypto-vault PACKAGE=@ragetrade/tricrypto-vault yarn typechain:repo-package",
    "typechain:gmx": "REPO=gmx-vault PACKAGE=@ragetrade/gmx-vault yarn typechain:repo-package",
    "typechain:uniswap:core": "REPO=uniswap-core PACKAGE=@uniswap/v3-core yarn typechain:repo-package",
    "typechain:uniswap:periphery": "REPO=uniswap-periphery PACKAGE=@uniswap/v3-periphery yarn typechain:repo-package"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@ragetrade/sdk",
  "author": "LeadDev",
  "homepage": "https://github.com/owner/project#readme",
  "module": "dist/sdk.esm.js",
  "size-limit": [
    {
      "path": "dist/sdk.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/sdk.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.5",
    "@typechain/ethers-v5": "^10.0.0",
    "@types/fs-extra": "^9.0.13",
    "dotenv": "^16.0.0",
    "fs-extra": "^10.0.0",
    "husky": "^7.0.4",
    "prettier": "^2.6.1",
    "size-limit": "^7.0.5",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typechain": "^8.0.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@ragetrade/core": "1.0.3-arbgoerli.1",
    "@ragetrade/gmx-vault": "^1.0.1",
    "@ragetrade/tricrypto-vault": "1.0.0-arbgoerli.1",
    "@uniswap/v3-sdk": "^3.8.2",
    "ethers": "^5.6.2",
    "ethers-repl": "^1.0.3",
    "fast-deep-equal": "^3.1.3"
  },
  "resolutions": {
    "tsdx/typescript": "^4.6.3",
    "@uniswap/v3-sdk/@uniswap/v3-staker": "^1.0.2"
  }
}
